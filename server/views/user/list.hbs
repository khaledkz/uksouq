<table class="table bg-primary table-bordered  mt-2">
    <thead class="table bg-dark text-white">
        <tr class="text-center align-middle">
            <th>#</th>
            <th>Username</th>
            <th>Update</th>

        </tr>
    </thead>
    <tbody class="table bg-info text-white">

        {{#each data}}
        <tr>
            <th scope="row" class="text-center align-middle">{{this._id}}</th>
            <td class="text-center align-middle">{{this.username}}</td>
            <td class="text-center align-middle">
                <a href="all/{{this._id}}">
                    <input class=" btn btn-sm btn-danger" type="button" value="View" />
                </a>
            </td>
        </tr>
        {{/each}}

    </tbody>
</table>